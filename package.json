{
	"name": "openfoodfacts-sveltekit",
	"version": "0.0.1",
	"private": true,
	"description": "Modern JavaScript frontend for Open Food Facts",
	"license": "AGPL-3.0",
	"repository": {
		"type": "git",
		"url": "git+https://github.com/openfoodfacts/openfoodfacts-explorer.git"
	},
	"engines": {
		"node": ">=18.0.0",
		"pnpm": ">=8.0.0"
	},
	"scripts": {
		"dev": "vite dev",
		"build": "vite build",
		"preview": "vite preview",
		"check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
		"check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
		"lint": "prettier --check . && eslint .",
		"format": "prettier --write .",
		"format:changed": "pretty-quick --staged",
		"api:folksonomy": "openapi-typescript https://api.folksonomy.openfoodfacts.org/openapi.json --output src/lib/api/folksonomy.d.ts",
		"api:prices": "openapi-typescript https://prices.openfoodfacts.net/api/schema --output src/lib/api/prices.d.ts",
		"api": "pnpm run api:folksonomy && pnpm run api:prices",
		"prepare": "husky",
		"pre-commit": "pnpm format"
	},
	"devDependencies": {
		"@crowdin/cli": "^4.11.0",
		"@eslint/eslintrc": "^3.3.1",
		"@eslint/js": "^9.35.0",
		"@fontsource-variable/plus-jakarta-sans": "^5.2.7",
		"@iconify-json/material-symbols": "^1.2.35",
		"@iconify-json/mdi": "^1.2.3",
		"@iconify-json/simple-icons": "^1.2.52",
		"@iconify/tailwind": "^1.2.0",
		"@iconify/tailwind4": "^1.0.6",
		"@openfoodfacts/openfoodfacts-nodejs": "2.0.0-alpha.17",
		"@sinnwerkstatt/sveltekit-matomo": "github:VaiTon/sveltekit-matomo",
		"@sveltejs/adapter-vercel": "^5.10.2",
		"@sveltejs/kit": "^2.39.1",
		"@sveltejs/vite-plugin-svelte": "^6.2.0",
		"@tailwindcss/postcss": "^4.1.13",
		"@tailwindcss/typography": "^0.5.16",
		"@types/cropperjs": "^1.3.3",
		"@types/leaflet": "^1.9.20",
		"@types/leaflet.markercluster": "^1.5.6",
		"@typescript-eslint/eslint-plugin": "^8.43.0",
		"@typescript-eslint/parser": "^8.43.0",
		"@vercel/speed-insights": "^1.2.0",
		"cropperjs": "^2.0.1",
		"cytoscape": "^3.33.1",
		"daisyui": "5.1.12",
		"eslint": "^9.35.0",
		"eslint-config-prettier": "^10.1.8",
		"eslint-plugin-svelte": "^3.12.3",
		"fuse.js": "^7.1.0",
		"globals": "^16.4.0",
		"html5-qrcode": "^2.3.8",
		"husky": "^9.1.7",
		"iso-639-1": "^3.1.5",
		"leaflet": "^1.9.4",
		"leaflet.markercluster": "^1.5.3",
		"openapi-fetch": "^0.14.0",
		"openapi-typescript": "^7.9.1",
		"postcss": "^8.5.6",
		"prettier": "^3.6.2",
		"prettier-plugin-svelte": "^3.4.0",
		"prettier-plugin-tailwindcss": "^0.6.14",
		"pretty-quick": "^4.2.2",
		"svelte": "^5.38.10",
		"svelte-check": "^4.3.1",
		"svelte-eslint-parser": "^1.3.2",
		"svelte-i18n": "^4.0.1",
		"svelte-local-storage-store": "^0.6.4",
		"sveltekit-i18n": "^2.4.2",
		"tailwindcss": "^4.1.13",
		"tailwindcss-opentype": "^1.2.0",
		"tslib": "^2.8.1",
		"typescript": "^5.9.2",
		"typescript-eslint": "^8.43.0",
		"vega": "^6.1.2",
		"vega-lite": "^6.3.1",
		"vite": "^7.1.5",
		"vite-plugin-static-copy": "^3.1.2"
	},
	"type": "module",
	"packageManager": "pnpm@10.15.1+sha512.34e538c329b5553014ca8e8f4535997f96180a1d0f614339357449935350d924e22f8614682191264ec33d1462ac21561aff97f6bb18065351c162c7e8f6de67",
	"pnpm": {
		"onlyBuiltDependencies": [
			"@tailwindcss/oxide",
			"@vercel/speed-insights",
			"es5-ext",
			"esbuild"
		],
		"overrides": {
			"esbuild@<=0.24.2": ">=0.25.0",
			"cookie@<0.7.0": ">=0.7.0",
			"devalue@<5.3.2": ">=5.3.2"
		}
	},
	"dependencies": {
		"@openfoodfacts/openfoodfacts-webcomponents": "^1.13.2",
		"@sentry/sveltekit": "^10.11.0",
		"@webcomponents/webcomponentsjs": "^2.8.0"
	}
}
