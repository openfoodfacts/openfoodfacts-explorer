{
	"name": "openfoodfacts-sveltekit",
	"version": "0.0.1",
	"private": true,
	"scripts": {
		"dev": "vite dev",
		"build": "vite build",
		"preview": "vite preview",
		"check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
		"check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
		"lint": "prettier --check . && eslint .",
		"format": "prettier --write .",
		"api:folksonomy": "openapi-typescript https://api.folksonomy.openfoodfacts.org/openapi.json --output src/lib/api/folksonomy/folksonomy.d.ts",
		"api:prices": "openapi-typescript https://prices.openfoodfacts.net/openapi.json --output src/lib/api/prices/prices.d.ts",
		"api": "pnpm run api:folksonomy && pnpm run api:prices"
	},
	"devDependencies": {
		"@iconify-json/mdi": "^1.1.64",
		"@iconify/tailwind": "^0.1.4",
		"@sveltejs/adapter-vercel": "^5.1.0",
		"@sveltejs/kit": "^2.5.3",
		"@sveltejs/vite-plugin-svelte": "^3.0.2",
		"@tailwindcss/typography": "^0.5.10",
		"@types/leaflet": "^1.9.8",
		"@typescript-eslint/eslint-plugin": "^6.20.0",
		"@typescript-eslint/parser": "^6.20.0",
		"@vercel/speed-insights": "^1.0.10",
		"@zoom-image/core": "^0.33.0",
		"@zoom-image/svelte": "^0.2.6",
		"autoprefixer": "^10.4.18",
		"daisyui": "^4.7.3",
		"eslint": "^8.57.0",
		"eslint-config-prettier": "^9.1.0",
		"eslint-plugin-svelte": "^2.35.1",
		"fuse.js": "^7.0.0",
		"html5-qrcode": "^2.3.8",
		"leaflet": "^1.9.4",
		"openapi-fetch": "^0.9.3",
		"openapi-typescript": "^6.7.4",
		"openfoodfacts-nodejs": "github:openfoodfacts/openfoodfacts-nodejs",
		"postcss": "^8.4.35",
		"prettier": "^3.2.5",
		"prettier-plugin-svelte": "^3.2.2",
		"prettier-plugin-tailwindcss": "^0.5.12",
		"svelte": "^4.2.12",
		"svelte-check": "^3.6.6",
		"svelte-local-storage-store": "^0.6.4",
		"tailwindcss": "^3.4.1",
		"tailwindcss-opentype": "^1.1.0",
		"tslib": "^2.6.2",
		"typescript": "^5.4.2",
		"vite": "^5.1.5"
	},
	"type": "module",
	"packageManager": "pnpm@8.10.5"
}
