{
	"name": "openfoodfacts-sveltekit",
	"version": "0.0.1",
	"private": true,
	"scripts": {
		"dev": "vite dev",
		"build": "vite build",
		"preview": "vite preview",
		"check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
		"check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
		"lint": "prettier --check . && eslint .",
		"format": "prettier --write .",
		"api:folksonomy": "openapi-typescript https://api.folksonomy.openfoodfacts.org/openapi.json --output src/lib/api/folksonomy/folksonomy.d.ts",
		"api:prices": "openapi-typescript https://prices.openfoodfacts.net/openapi.json --output src/lib/api/prices/prices.d.ts",
		"api": "pnpm run api:folksonomy && pnpm run api:prices"
	},
	"devDependencies": {
		"@iconify-json/mdi": "^1.1.55",
		"@iconify/tailwind": "^0.1.3",
		"@sveltejs/adapter-auto": "^2.1.1",
		"@sveltejs/kit": "^1.27.6",
		"@tailwindcss/typography": "^0.5.10",
		"@types/leaflet": "^1.9.8",
		"@typescript-eslint/eslint-plugin": "^6.12.0",
		"@typescript-eslint/parser": "^6.12.0",
		"autoprefixer": "^10.4.16",
		"daisyui": "^4.4.2",
		"eslint": "^8.54.0",
		"eslint-config-prettier": "^9.0.0",
		"eslint-plugin-svelte": "^2.35.0",
		"fuse.js": "^7.0.0",
		"html5-qrcode": "^2.3.8",
		"leaflet": "^1.9.4",
		"openapi-fetch": "^0.8.1",
		"openapi-typescript": "^6.7.1",
		"postcss": "^8.4.31",
		"prettier": "^3.1.0",
		"prettier-plugin-svelte": "^3.1.0",
		"prettier-plugin-tailwindcss": "^0.5.7",
		"svelte": "^4.2.7",
		"svelte-check": "^3.6.0",
		"svelte-local-storage-store": "^0.6.4",
		"tailwindcss": "^3.3.5",
		"tailwindcss-opentype": "^1.1.0",
		"tslib": "^2.6.2",
		"typescript": "^5.3.2",
		"vite": "^4.5.0"
	},
	"type": "module",
	"packageManager": "pnpm@8.10.5"
}
