{
	"name": "openfoodfacts-sveltekit",
	"version": "0.0.1",
	"private": true,
	"description": "Modern JavaScript frontend for Open Food Facts",
	"license": "AGPL-3.0",
	"repository": {
		"type": "git",
		"url": "git+https://github.com/openfoodfacts/openfoodfacts-explorer.git"
	},
	"engines": {
		"node": ">=18.0.0",
		"pnpm": ">=8.0.0"
	},
	"scripts": {
		"dev": "vite dev",
		"build": "vite build",
		"preview": "vite preview",
		"check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
		"check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
		"lint": "prettier --check . && eslint .",
		"format": "prettier --write .",
		"format:changed": "pretty-quick --staged",
		"api:folksonomy": "openapi-typescript https://api.folksonomy.openfoodfacts.org/openapi.json --output src/lib/api/folksonomy.d.ts",
		"api:prices": "openapi-typescript https://prices.openfoodfacts.net/api/schema --output src/lib/api/prices.d.ts",
		"api": "pnpm run api:folksonomy && pnpm run api:prices",
		"prepare": "husky",
		"pre-commit": "pnpm format"
	},
	"devDependencies": {
		"@crowdin/cli": "^4.11.0",
		"@eslint/eslintrc": "^3.3.1",
		"@eslint/js": "^9.37.0",
		"@fontsource-variable/plus-jakarta-sans": "^5.2.8",
		"@iconify-json/material-symbols": "^1.2.41",
		"@iconify-json/mdi": "^1.2.3",
		"@iconify-json/simple-icons": "^1.2.54",
		"@iconify/tailwind": "^1.2.0",
		"@iconify/tailwind4": "^1.0.6",
		"@openfoodfacts/openfoodfacts-nodejs": "2.0.0-alpha.19",
		"@sinnwerkstatt/sveltekit-matomo": "github:VaiTon/sveltekit-matomo",
		"@sveltejs/adapter-node": "^5.3.3",
		"@sveltejs/adapter-vercel": "^6.0.0",
		"@sveltejs/kit": "^2.46.4",
		"@sveltejs/vite-plugin-svelte": "^6.2.1",
		"@tailwindcss/postcss": "^4.1.14",
		"@tailwindcss/typography": "^0.5.19",
		"@types/cropperjs": "^1.3.3",
		"@types/leaflet": "^1.9.21",
		"@types/leaflet.markercluster": "^1.5.6",
		"@typescript-eslint/eslint-plugin": "^8.46.0",
		"@typescript-eslint/parser": "^8.46.0",
		"@vercel/speed-insights": "^1.2.0",
		"cropperjs": "^2.0.1",
		"cytoscape": "^3.33.1",
		"daisyui": "5.2.3",
		"eslint": "^9.37.0",
		"eslint-config-prettier": "^10.1.8",
		"eslint-plugin-svelte": "^3.12.4",
		"fuse.js": "^7.1.0",
		"globals": "^16.4.0",
		"html5-qrcode": "^2.3.8",
		"iso-639-1": "^3.1.5",
		"jsbarcode": "^3.12.1",
		"leaflet": "^1.9.4",
		"leaflet.markercluster": "^1.5.3",
		"openapi-fetch": "^0.14.1",
		"openapi-typescript": "^7.9.1",
		"postcss": "^8.5.6",
		"prettier": "^3.6.2",
		"prettier-plugin-svelte": "^3.4.0",
		"prettier-plugin-tailwindcss": "^0.6.14",
		"pretty-quick": "^4.2.2",
		"svelte": "^5.39.11",
		"svelte-check": "^4.3.3",
		"svelte-eslint-parser": "^1.3.3",
		"svelte-i18n": "^4.0.1",
		"svelte-local-storage-store": "^0.6.4",
		"sveltekit-i18n": "^2.4.2",
		"tailwindcss": "^4.1.14",
		"tailwindcss-opentype": "^1.2.0",
		"tslib": "^2.8.1",
		"typescript": "^5.9.3",
		"typescript-eslint": "^8.46.0",
		"vega": "^6.2.0",
		"vega-lite": "^6.4.1",
		"vite": "^7.1.9",
		"vite-plugin-static-copy": "^3.1.3"
	},
	"type": "module",
	"packageManager": "pnpm@10.17.0+sha512.fce8a3dd29a4ed2ec566fb53efbb04d8c44a0f05bc6f24a73046910fb9c3ce7afa35a0980500668fa3573345bd644644fa98338fa168235c80f4aa17aa17fbef",
	"pnpm": {
		"onlyBuiltDependencies": [
			"@tailwindcss/oxide",
			"@vercel/speed-insights",
			"es5-ext",
			"esbuild"
		],
		"overrides": {
			"esbuild@<=0.24.2": ">=0.25.0",
			"cookie@<0.7.0": ">=0.7.0",
			"devalue@<5.3.2": ">=5.3.2"
		}
	},
	"dependencies": {
		"@openfoodfacts/openfoodfacts-webcomponents": "^1.14.2",
		"@sentry/sveltekit": "^10.19.0",
		"@webcomponents/webcomponentsjs": "^2.8.0",
		"husky": "^9.1.7"
	}
}
